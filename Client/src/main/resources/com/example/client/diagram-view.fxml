<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.controllers.DiagramView">
   <children>
      <PieChart fx:id="pieChart" layoutX="5.0" layoutY="11.0" legendSide="TOP" legendVisible="false" prefHeight="388.0" prefWidth="589.0" title="Аналитика размеров получаемых студентами стипендий" />
      <Button alignment="CENTER" layoutX="14.0" layoutY="369.0" mnemonicParsing="false" onAction="#goBack" prefHeight="17.0" prefWidth="47.0" style="-fx-background-color: #c5c3c6;" text="Выйти" textAlignment="CENTER" textFill="#4c5c68" underline="true">
         <font>
            <Font name="Arial Bold" size="9.0" />
         </font>
      </Button>
      <Button fx:id="save" alignment="CENTER" layoutX="440.0" layoutY="369.0" mnemonicParsing="false" onAction="#saveReport" prefHeight="18.0" prefWidth="145.0" style="-fx-background-color: #73628A;" text="Сохранить отчет" textAlignment="CENTER" textFill="#fcf6ff" underline="true">
         <font>
            <Font name="Arial Bold" size="9.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
